{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "start": {
      "dependsOn": ["^start"],
      "outputs": ["dist/**"],
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": ["dist/**"],
      "cache": false
    }
  },
  "noUpdateNotifier": true
}
